	 this.quantityInStock = quantityInStock;
	 
	 this.price = price;
	 
	 this.supplierID = supplierID;
	 
 }
 
 public int getID() {
	 
	 return Id;
	 
 }
 
 public updateQuantity(int newQuantity){
	 
 }
 
 public double getPrice() {
	 
 }
 
 public String toString() {
	 return "Tool ID: " + Id + "\nName: " + name + "\naQuantity in Stock: " + quantity + "\nPrice: " + price + "\nSupplier ID: " + supplierId;
 }
 
}
